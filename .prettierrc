{
  "singleQuote": true,
  "semi": true,
  "printWidth": 80,
  "bracketSameLine": false,
  "quoteProps": "as-needed",
  "tabWidth": 2,
  "trailingComma": "all",
  "plugins": [
    "@ianvs/prettier-plugin-sort-imports",
    "prettier-plugin-packagejson"
  ],
  "importOrder": [
    "<TYPES>^(node:)",
    "<TYPES>",
    "<TYPES>^[.]",
    "<BUILTIN_MODULES>",
    "<THIRD_PARTY_MODULES>",
    "^(@workspace)(/.*)$",
    "^[.]"
  ]
}
